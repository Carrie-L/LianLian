<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.carrie.lldiary">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".App"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="io.fabric.ApiKey"
            android:value="e9711e47de2f01e87b1b2a761338687e93c10e5d" />

        <activity android:name=".Person" />
        <activity
            android:name=".base.BaseActivity"
            android:label="@string/title_activity_base" />
        <activity android:name=".activity.DiaryActivity" />
        <activity android:name=".activity.DiaryEditActivity"></activity>
    </application>

</manifest>